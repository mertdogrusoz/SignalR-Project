@model SignalRWebUI.Dtos.ContactDtos.UpdateContactDto
@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}




<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İletişim Güncelleme İşlemleri</h4>
		<div class="row">
			<div class="col-md-12">
				<form method="post">
					<div class="card">
						<div class="card-header">
							<div class="card-title"> İletişim Bilgileri Güncelleme</div>
						</div>
						<input type="hidden" asp-for="ContactId" />
						<div class="card-body">
							<div class="form-group">
								<label for="Location">Lokasyon Adını Giriniz</label>
								<input type="text" class="form-control" asp-for="Location" id="Location">
							</div>
						</div>

						<div class="card-body">
							<div class="form-group">
								<label for="PhoneNumber">Telefon Numarasını Giriniz</label>
								<input type="text" class="form-control" asp-for="PhoneNumber" id="PhoneNumber">
							</div>
						</div>

						<div class="card-body">
							<div class="form-group">
								<label for="Mail">Mail Giriniz</label>
								<input type="text" class="form-control" asp-for="Mail" id="Mail">
							</div>
						</div>

						<div class="card-body">
							<div class="form-group">
								<label for="FooterDescription">Açıklama Giriniz</label>
								<input type="text" class="form-control" asp-for="FooterDescription" id="FooterDescription">
							</div>
						</div>



						<div class="card-action">
							<button class="btn btn-primary">İletişim Bilgilerini Güncelle</button>
						</div>
					</div>
				</form>
			</div>

		</div>
	</div>
</div>

